{
	"servers": {
		"MongoDB": {
			"command": "npx",
			"args": [
				"-y",
				"mongodb-mcp-server@latest"
			],
			"envFile": "${workspaceFolder}/.env",
			"type": "stdio"
		},
		"chrome-devtools": {
			"command": "npx",
			"args": [
				"chrome-devtools-mcp@latest"
			],
			"type": "stdio"
		},
		"sequential-thinking": {
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"type": "stdio"
		},
		"Vercel": {
			"url": "https://mcp.vercel.com",
			"type": "http"
		}
	},
	"inputs": []
}